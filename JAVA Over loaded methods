public class Main {
    public static void main(String[] args) {
        int x= add(1,2,9);


    }
    static int add(int a,int b){
        System.out.println("over load methohd");
        return a+b;

    }
    static int add(int a,int b,int c){
        System.out.println("over load methohd 2");
        return a+b+c;

    }
    static int add(int a,int b,int c,int d){
        System.out.println("over load methohd 3");
        return a+b+c+d;
    }
}
