public class Human {
    String name;
    int age;
    double weight;

    Human(String name,int age,double weight){
        this.name=name;
        this.age=age;
        this.weight=weight;

    }
    void eat(){
        System.out.println(this.name+"is eating");
    }
}


public class Main {
public static void main(String[] args){
    Human human=new Human("rick",67,70);
  human.eat();


}
}
